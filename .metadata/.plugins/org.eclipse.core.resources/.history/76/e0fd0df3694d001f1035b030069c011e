package com.aurionpro.test;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class StringTest {
	public static void main(String[] args) {
		List<String> names = Arrays.asList("Jayesh","Nimesh","Mark", "Mahesh", "Ramesh");
		
		
		System.out.println("1.First 3 students sorted in ascending order.");
		List<String> sorted3Names =names.stream().sorted().limit(3).collect(Collectors.toList());
		sorted3Names.forEach((name)->System.out.println("The 3 Students Sorted in ascending order: "+ sorted3Names));
		
		System.out.println();
		
	}

}
