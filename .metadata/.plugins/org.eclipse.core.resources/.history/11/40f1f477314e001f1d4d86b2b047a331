package com.aurionpro.ocp.model;

import com.aurionpro.ocpviolation.model.Shape;

public class Rectangle implements Shape{
	private int width;
	private int height;
	
	public Rectangle(int width, int height) {
		super();
		this.width = width;
		this.height = height;
	}

	@Override
	public void area() {
		return width*height;
	}
	
	


}
