package com.aurionpro.model;

public class Student {
	
	private static final String FILE_PATH ="D:\\Aurionpro_training\\Assignment_29_07_24\\src\\com\\aurionpro\\model\\studentdetail.txt";
	
	public static void main(String[] args) {
		System.out.println("Reading with FileReader: ");
		FileReaderExample fr = new FileReaderExample(FILE_PATH);
		fr.readLines();
		
		System.out.println("\nReading with Stream: ");
		StreamReaderExample sr = new StreamReaderExample(FILE_PATH);
		sr.readLines();
	}

}
