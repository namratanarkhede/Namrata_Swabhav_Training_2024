package com.aurionpro.dipviolation.model;

import com.aurionpro.dip.model.DBLogger;

public class TaxCalulator {
	private ILogger logger;
	

	public TaxCalulator(ILogger logger) {
		super();
		this.logger = logger;
	}


	public ILogger getLogger() {
		return logger;
	}


	public void setLogger(ILogger logger) {
		this.logger = logger;
	}


	public int calculateTax(int amount, int rate){
		int tax =0;
		try {
			tax = amount/rate;
			System.out.println(tax);
		}
		catch(Exception e) {
			new logger().log("Divide by zero");
		}
		return tax;
	}
	
	
	
	

}
