package com.aurionpro.structural.test;

import com.aurionpro.structural.model.IHat;
import com.aurionpro.structural.model.PremiumHat;
import com.aurionpro.structural.model.StandardHat;

public class HatDecoratorTest {

	public static void main(String[] args) {
		IHat premiumHat =new PremiumHat();
		System.out.println("Premium hat details");
		premiumHat.getName();
		premiumHat.getPrice();
		premiumHat.getDescription();
		
		IHat standradHat = new StandardHat();
		System.out.println("\nStandard hat details");
		standradHat.getName();
		standradHat.getPrice();
		standradHat.getDescription();
	}
}
