package com.aurionpro.dipviolation.model;

import com.aurionpro.dip.model.DBLogger;

public class TaxCalulator {
	private ILogger iLogger;
	
	public int calculateTax(int amount, int rate){
		int tax =0;
		try {
			tax = amount/rate;
			System.out.println(tax);
		}
		catch(Exception e) {
			new DBLogger().log("Divide by zero");
		}
		return tax;
	}
	
	
	
	

}
