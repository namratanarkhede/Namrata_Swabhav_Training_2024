package com.aurionpro.creational.builder.model;

public class Car implements ICar{

	private final String name;
	
	private Car(String name) {
		this.name = name;
	}
	
	@Override
	public void start() {
		System.out.println(name + "is starting.");
	}

	@Override
	public void stop() {
		System.out.println(name + "is Stopped.");
	}

	@Override
	public String getName() {
		return name;
	}
	
	public static CarBuilder builder(String name) {
		return new CarBuilder(name);
	}

}
