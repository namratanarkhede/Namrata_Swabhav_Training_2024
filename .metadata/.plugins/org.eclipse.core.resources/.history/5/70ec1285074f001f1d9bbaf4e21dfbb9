package com.aurionpro.creational.factory.model;

public class AccountFactory {
	public static IAccount makeAccount(int accountNo, String name, double balance, double minBalance, AccountType accountType) {
		
		IAccount account =null ;
		if(accountType == AccountType.SAVING)
			accountType = new SavingAccount(accountNo, name, balance, minBalance);
		if(accountType == AccountType.CURRENT)
			accountType = new CurrentAccount(accountNo, name , balance, overDraftLimit);
		return accountType;
	}

}
